<%- include("partial/header") -%>
<body class="givePadding">
    <form action="/" method="post">
        <div class="form-group">
            <label for="usr">Title</label>
            <input type="text" name="composeTitle" class="form-control" id="usr" required autofocus>
        </div>
        <div class="form-group">
            <label for="exampleFormControlTextarea1">Post</label>
            <textarea class="form-control" name="composePost" id="exampleFormControlTextarea1" rows="3"></textarea>
        </div>
        <h1>Main Heading ?</h1>
        <input type="checkbox" id="Yes" name="Yes" value="Bike">
        <label for="Yes">Yes</label><br>
        <input type="checkbox" id="No" name="No" value="Car">
        <label for="No">No</label><br>
        <!-- <input type="submit" value="Submit"> -->
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</body>